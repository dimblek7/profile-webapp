    "yarn": "^1.3.2"
  },
  "scripts": {
    "start": "ns-react-scripts start",
    "build": "ns-react-scripts build",
    "test": "ns-react-scripts test --env=jsdom --colors",
    "test:coverage": "ns-react-scripts test --env=jsdom --ci --runInBand --coverage --colors",
    "format:js": "prettier --config .prettierrc \"src/**/*.js\" --write",
    "lint:js": "eslint --config .eslintrc \"src/**/*.js\"",
    "lint:style": "stylelint \"src/**/*.scss\"",
    "precommit": "lint-staged",
    "prepush": "npm run test:coverage",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "start-json-server": "json-server --watch db.json --port 3001",
    "test-with-coverage": "ns-react-scripts test --env=jsdom --ci --runInBand --coverage --colors && cat ./coverage/lcov.info | codacy-coverage"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.16",
    "@storybook/addon-knobs": "^3.2.16",
    "@storybook/addon-links": "^3.2.16",
    "@storybook/addon-notes": "^3.2.16",
    "@storybook/react": "^3.2.16",
    "enzyme": "^3.4.4",
    "enzyme-adapter-react-16": "^1.2.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "husky": "^0.14.3",
    "lint-staged": "^5.0.0",
    "prettier": "^1.8.2",
    "react-test-renderer": "^16.4.1",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "stylelint": "^8.3.1",
    "stylelint-config-standard": "^18.0.0"
  },
  "lint-staged": {
    "*.js": [
      "npm run format:js",
      "npm run lint:js",
      "git add"
    ],
    "*.scss": [
      "npm run lint:style",
      "git add"
    ]
  }
}
