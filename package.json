    "react-dom": "^16.2.0",
    "react-easy-chart": "^1.0.0",
    "react-ellipsis-text": "^1.0.0",
    "react-fittext": "^1.0.0",
    "react-ga": "^2.5.3",
    "react-image-cropper": "^1.3.0",
    "react-intl": "^2.4.0",
    "react-loadable": "^5.3.1",
    "react-player": "^1.6.4",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-select": "^1.2.1",
    "react-switch": "^2.3.0",
    "react-toastify": "^3.0.0",
    "reactstrap": "^5.0.0-alpha.4",
